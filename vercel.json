{
  "builds": [
    { "src": "server/index.ts", "use": "@vercel/node" },
    { "src": "client/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server/index.ts" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "outputDirectory": "dist/public"
}
{
  "redirects": [
    { "source": "/", "destination": "https://www.calculatethis.org", "permanent": true }
  ]
}
